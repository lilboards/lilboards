{
  "name": "lilboards",
  "version": "4.8.0",
  "description": "Create boards, columns, and items.",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "firebase:deploy:database": "firebase deploy --only database",
    "firebase:emulators": "firebase emulators:start",
    "firebase:init": "firebase init",
    "firebase:login": "firebase login",
    "firebase:logout": "firebase logout",
    "lint": "yarn lint:ts && yarn lint:tsc",
    "lint:fix": "yarn lint:ts --fix",
    "lint:ts": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "husky install",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:ci": "CI=true yarn test --coverage --colors"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@reach/router": "^1.3.4",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.3.0",
    "@types/debounce": "^1.2.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/reach__router": "^1.3.9",
    "@types/react": "^17.0.30",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.19",
    "debounce": "^1.2.1",
    "firebase": "^8.10.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-firebaseui": "^5.0.2",
    "react-redux": "^7.2.5",
    "react-scripts": "4.0.3",
    "typescript": "4.4.4",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "eslint-plugin-prettier": "^3.4.1",
    "firebase-tools": "^9.20.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1"
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1"
  },
  "private": true,
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
