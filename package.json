    "firebase:deploy:database": "firebase deploy --only database",
    "firebase:emulators": "firebase emulators:start --only auth,database",
    "firebase:init": "firebase init",
    "firebase:login": "firebase login",
    "firebase:logout": "firebase logout",
    "lint": "npm run lint:ts && npm run lint:tsc",
    "lint:fix": "npm run lint:ts -- --fix",
    "lint:ts": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "husky install",
    "size": "size-limit",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:ci": "CI=true npm test -- --ci --coverage --colors"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.15",
    "@mui/material": "5.14.15",
    "@reduxjs/toolkit": "1.9.7",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.5.1",
    "@types/debounce": "1.2.3",
    "@types/jest": "29.5.7",
    "@types/node": "20.8.10",
    "@types/react": "18.2.33",
    "@types/react-beautiful-dnd": "13.1.6",
    "@types/react-dom": "18.2.14",
    "debounce": "1.2.1",
    "firebase": "10.5.2",
    "firebaseui": "6.1.0",
    "react": "18.2.0",
    "react-beautiful-dnd": "13.1.1",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "react-router-dom": "6.17.0",
    "react-scripts": "5.0.1",
    "typescript": "5.1.6",
    "web-vitals": "3.5.0"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "18.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "2.2.0",
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@size-limit/preset-app": "10.0.1",
    "cypress": "13.4.0",
    "cypress-cucumber-steps": "3.15.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-tsdoc": "0.2.17",
    "firebase-tools": "12.7.0",
    "husky": "8.0.3",
    "lint-staged": "15.0.2",
    "prettier": "3.0.3",
    "size-limit": "10.0.1"
  },
  "engines": {
    "node": "18"
  },
  "private": true,
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
