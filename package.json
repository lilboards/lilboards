{
  "name": "lilboards",
  "version": "4.20.0",
  "description": "Create boards, columns, and items.",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "firebase:deploy:database": "firebase deploy --only database",
    "firebase:emulators": "firebase emulators:start",
    "firebase:init": "firebase init",
    "firebase:login": "firebase login",
    "firebase:logout": "firebase logout",
    "lint": "yarn lint:ts && yarn lint:tsc",
    "lint:fix": "yarn lint:ts --fix",
    "lint:ts": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "husky install",
    "size": "size-limit",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:ci": "CI=true yarn test --coverage --colors"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.3",
    "@mui/material": "^5.8.3",
    "@reach/router": "^1.3.4",
    "@reduxjs/toolkit": "^1.8.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/debounce": "^1.2.1",
    "@types/jest": "^28.1.1",
    "@types/node": "^18.0.0",
    "@types/reach__router": "^1.3.10",
    "@types/react": "^18.0.12",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.24",
    "debounce": "^1.2.1",
    "firebase": "^9.8.3",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.1.0",
    "react-firebaseui": "^6.0.0",
    "react-redux": "^8.0.2",
    "react-scripts": "5.0.1",
    "typescript": "4.7.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@size-limit/preset-app": "^7.0.8",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tsdoc": "^0.2.16",
    "firebase-tools": "^11.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.7.0",
    "size-limit": "^7.0.8"
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1"
  },
  "private": true,
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
