{
  "name": "lilboards",
  "version": "4.32.0",
  "description": "Create boards, columns, and items.",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "build": "react-scripts build",
    "cypress:open": "cypress open",
