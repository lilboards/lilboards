{
  "name": "lilboards",
  "version": "4.12.0",
  "description": "Create boards, columns, and items.",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "firebase:deploy:database": "firebase deploy --only database",
    "firebase:emulators": "firebase emulators:start",
    "firebase:init": "firebase init",
    "firebase:login": "firebase login",
    "firebase:logout": "firebase logout",
    "lint": "yarn lint:ts && yarn lint:tsc",
    "lint:fix": "yarn lint:ts --fix",
    "lint:ts": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "husky install",
    "size": "size-limit",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:ci": "CI=true yarn test --coverage --colors"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.4",
    "@mui/material": "^5.2.4",
    "@reach/router": "^1.3.4",
    "@reduxjs/toolkit": "^1.7.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/debounce": "^1.2.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.0",
    "@types/reach__router": "^1.3.9",
    "@types/react": "^17.0.37",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "debounce": "^1.2.1",
    "firebase": "^9.6.1",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-firebaseui": "^6.0.0",
    "react-redux": "^7.2.6",
    "react-scripts": "4.0.3",
    "typescript": "4.5.4",
    "web-vitals": "^2.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@size-limit/preset-app": "^7.0.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "firebase-tools": "^9.23.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "size-limit": "^7.0.4"
  },
  "engines": {
    "node": ">=10",
    "yarn": ">=1"
  },
  "private": true,
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
