{
  "extends": ["react-app", "react-app/jest"],
  "plugins": ["prettier", "simple-import-sort", "tsdoc"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "no-console": "error",
    "no-debugger": "error",
    "prettier/prettier": "error",
    "simple-import-sort/exports": "error",
    "simple-import-sort/imports": "error",
    "tsdoc/syntax": "error"
  }
}
